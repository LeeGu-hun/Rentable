<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:mvc="http://www.springframework.org/schema/mvc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/mvc
		http://www.springframework.org/schema/mvc/spring-mvc.xsd">

	<bean class="controller.MainController">
		<property name="mainService" ref="mainService"></property>

	</bean>

	<bean class="controller.JoinController">
		<property ref="joinService" name="joinService" />
		<property name="mainService" ref="mainService"/>
	</bean>

	<bean class="controller.ItemController">
		<property ref="itemService" name="itemService" />
		<property ref="prodService" name="prodService" />

	</bean>

	<bean class="controller.loginController">
		<property name="memberService" ref="memberService"></property>
	</bean>


	<bean class="controller.UserItemController">
		<property name="useritemService" ref="userItemService"></property>

	</bean>
	<bean class="controller.ProdController">
		<property name="mainService" ref="mainService"></property>
		<property ref="prodService" name="prodService" />

	</bean>
</beans>