<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="useritemlistSQL">


	<select id="myList" resultType="bean.bean_rent_products">
		select
		RP_itemnum,RP_itemname,RP_usernum
		from
		rent_products
		order by rp_itemnum desc
	</select>



</mapper>